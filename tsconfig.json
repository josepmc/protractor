{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "built/",
    "baseUrl": ".",
    "paths": {
      "*": [
        "*"
      ],
      "webdriver-manager": [
        "node_modules/@immoweb/webdriver-manager"
      ]
    }
  },
  "exclude": [
    "built",
    "node_modules",
    "testapp",
    "website",
    "scripts",
    "exampleTypescript",
    "spec/**/*"
  ]
}